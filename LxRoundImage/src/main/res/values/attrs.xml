<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LxImage">
        <attr name="shape" format="integer">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="roundRadius" format="dimension" />
        <attr name="LeftTopRadius" format="dimension" />
        <attr name="RightTopRadius" format="dimension" />
        <attr name="LeftButtomRadius" format="dimension" />
        <attr name="RightButtomRadius" format="dimension" />

    </declare-styleable>


</resources>

